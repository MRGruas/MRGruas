{
  "AutoPreview.center": true,
  "AutoPreview.packageManager": "",
  "AutoPreview.serverURL": "",
  "[code-text-binary]": {},
  "[coffeescript]": {
    "diffEditor.ignoreTrimWhitespace": false
  },
  "[csharp]": {
    "editor.maxTokenizationLineLength": 2500
  },
  "[css]": {
    "[jsonc]": {
      "editor.quickSuggestions": {
        "strings": true
      },
      "editor.suggest.insertMode": "replace"
    },
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[git-commit]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.rulers": [72],
    "workbench.editor.restoreViewState": false
  },
  "[github-actions-workflow]": {
    "breadcrumbs.showArrays": true
  },
  "[html]": {
    "editor.suggest.insertMode": "replace"
  },
  "[javascript]": {
    "editor.maxTokenizationLineLength": 2500
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[jsonl]": {
    "editor.autoIndent": "advanced"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[scss]": {
    "editor.suggest.insertMode": "replace"
  },
  "audioCues.diffLineDeleted": "on",
  "auto-correct.dictionary": [
    {
      "languages": ["*"],
      "useLargeList": false,
      "words": {}
    },
    {
      "languages": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "words": {
        "cosnt": "const",
        "functoin": "function"
      }
    }
  ],
  "autoprefixer.findExternalAutoprefixer": true,
  "autoprefixer.formatOnSave": false,
  "autoprefixer.ignoreFiles": [],
  "autoprefixer.options": {},
  "codeQL.variantAnalysis.controllerRepo": "https://github.com/MRGruas/MRGruas.github.io",
  "css.completion.completePropertyWithSemicolon": true,
  "css.completion.triggerPropertyValueCompletion": true,
  "css.customData": [],
  "css.enabledLanguages": ["html"],
  "css.format.braceStyle": "expand",
  "css.format.enable": true,
  "css.format.maxPreserveNewLines": null,
  "css.format.newlineBetweenRules": true,
  "css.format.newlineBetweenSelectors": true,
  "css.hover.documentation": true,
  "css.styleSheets": [],
  "csstree.enable": true,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.colorDecorators": true,
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "extensions.autoCheckUpdates": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "flutter-auto-insert-comma.activationFiles": ["dart"],
  "git.autofetch": true,
  "html.autoClosingTags": true,
  "html.autoCreateQuotes": true,
  "html.format.enable": true,
  "json.format.enable": true,
  "livePreview.autoRefreshPreview": "true",
  "livePreview.notifyOnOpenLooseFile": true,
  "livePreview.openPreviewTarget": "External Browser",
  "npm-intellisense.importQuotes": "'",
  "npm-intellisense.scanDevDependencies": false,
  "npm.autoDetect": "on",
  "npm.packageManager": "auto",
  "prettier.cssEnable": ["css", "less", "scss"],
  "redhat.telemetry.enabled": true,
  "remote.downloadExtensionsLocally": true,
  "remoteHub.commitDirectlyWarning": "off",
  "remoteHub.uncommittedChangesOnEntry": "none",
  "scss.format.enable": true,
  "scss.validate": true,
  "security.workspace.trust.enabled": true,
  "stylelint.codeAction.disableRuleComment": {
    "location": "separateLine"
  },
  "stylelint.config": {},
  "stylelint.configFile": "",
  "stylelint.customSyntax": "",
  "stylelint.enable": true,
  "stylelint.packageManager": "npm",
  "stylelint.reportInvalidScopeDisables": false,
  "stylelint.snippet": ["css", "less", "postcss"],
  "stylelint.stylelintPath": "",
  "window.zoomLevel": -2,
  "workbench.settings.editor": "json",
  "fixJson.indentationSpaces": null,
  "[http]": {
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    }
  }
}
